{
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "version": "1.0.0",
    "description": "A simple React app as a Chrome extension",
    "action": {
      "default_popup": "index.html",
      "width": 400
    },
    "permissions": ["tabs", "storage", "activeTab", "scripting"],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    }
    
    
  }
